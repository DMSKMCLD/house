.box{
  width: 100%;
  height: auto;
}
.nodata{
  width: 100%;
}
.list{
  border-radius: 50rpx;
  margin-top: 50rpx;
  width: 100%;
  height: 617rpx;
  background: #ff7f50;
  position: relative;
  color: black;
}
.time{
  margin-left: 2%;
  width: 50%;
  height: 50rpx;
  line-height: 50rpx;
  border-radius: 15rpx;
  text-align: center;
  border-radius: 66rpx;
  background: #f0f0f0;
}
.detail{
  width: 100%;
  height: 378rpx;
  position: relative;
}
.detimg{
  width: 100%;
  height: 200rpx;
  text-align: center;
  outline: 3rpx solid #ee9572;
}
.detimgs{
  border: 1rpx solid orange;
  color: cornsilk;
  width: 100%;
  height: 30rpx;
  line-height: 30rpx;
  text-align: center;
  border-radius: 20rpx;
}
.detimg>image{
  height: 170rpx;
  float: left;
  outline: 1rpx solid #ff8c69;
}
.detcon{
  width: 70%;
  height: 100rpx;
  line-height: 100rpx;
  position: absolute;
  left: 0;
  bottom: 0;
  text-align: center;
}
.detlabel{
  width: 30%;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  position: absolute;
  right: 0;
  bottom: 0;
  font-size: 26rpx;
}
.infor{
  position: absolute;
  width: 76%;
  height: 188rpx;
  left: 0;
  bottom: 0;
}
.inforavt{
  text-align: center;
  width: 26%;
  height: 178rpx;
  position: absolute;
  left: 1%;
  bottom: 10rpx;
  font-size: 28rpx;
}
.inforavt>image{
  border-radius: 68rpx;
  width: 100%;
  height: 136rpx;
  position: absolute;
  left: 0;
  bottom: 0; 
  /* outline: 1rpx solid #ccffff; */
}
.inforname{
  width: 100%;
  border-radius: 20rpx;
  background: #f2f2f2;
  height: 89rpx;
  line-height: 89rpx;
  position: absolute;
  left: 29%;
  top: 0;
  text-align: center;
}
.inforphone{
  width: 72%;
  height: 89rpx;
  line-height: 75rpx;
  position: absolute;
  text-align: center;
  right: 0;
  bottom: 12rpx;
}
.through{
  position: absolute;
  right: 48%;
  bottom: 10rpx;
}
.rejected{
  position: absolute;
  right: 10%;
  bottom: 10rpx;
}