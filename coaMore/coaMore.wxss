.idcard{
  width: 100%;
  height: 270rpx;
}
.idcard>text{
  color: red;
  width: 100%;
  height: 50rpx;
  line-height: 50rpx;
  text-align: center;
  display: block;
}
.cardimg{
  width: 100%;
  height: 50rpx;
}
.cardimg>image{
  width: 48%;
  height: 200rpx;
  margin-left: 10rpx;
  float: left;
  outline: 3rpx solid black;
  margin-top: 5rpx;
}
.idcards{
  width: 100%;
  height: 270rpx;
}
.idcards>text{
  color: red;
  width: 100%;
  height: 50rpx;
  line-height: 50rpx;
  text-align: center;
  display: block;
}
.cardimgs{
  width: 100%;
  height: 50rpx;
}
.cardimgs>image{
  width: 30%;
  height: 200rpx;
  margin-left: 10rpx;
  float: left;
  margin-top: 5rpx;
}
.through>button{
  margin-top: 10rpx;
}
.rejected>button{
  margin-top: 10rpx;
}